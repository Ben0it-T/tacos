<!DOCTYPE html>
<html lang="fr">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="robots" content="noindex, nofollow">

    <link rel="icon" type="image/png" href="{{ basePath }}/favicon-96x96.png" sizes="96x96">
    <link rel="icon" type="image/svg+xml" href="{{ basePath }}/favicon.svg">
    <link rel="shortcut icon" href="{{ basePath }}/favicon.ico">
    <link rel="apple-touch-icon" sizes="180x180" href="{{ basePath }}/apple-touch-icon.png">
    <meta name="apple-mobile-web-app-title" content="Tacos">
    <link rel="manifest" href="{{ basePath }}/manifest.json">

    <title>Tacos | {% block title %}{% endblock %}</title>

    <link href="{{ basePath }}/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="{{ basePath }}/lib/bootstrap-icons/bootstrap-icons.min.css" rel="stylesheet">
    <link href="{{ basePath }}/lib/datatables/dataTables.bootstrap5.min.css" rel="stylesheet">
    {% block css %}{% endblock %}
    <link href="{{ basePath }}/css/theme.css" rel="stylesheet">
</head>
<body class="border-top border-primary" basepath="{{ basePath is empty ? "" : basePath }}" >
    {% block navbar %}{% endblock %}
    <div class="container-fluid container-max-width">
        {% block content %}{% endblock %}
    </div>

    <script nonce="{{ nonce }}" src="{{ basePath }}/lib/jquery/jquery-3.7.1.min.js"></script>
    <script nonce="{{ nonce }}" src="{{ basePath }}/lib/popper/popper.min.js"></script>
    <script nonce="{{ nonce }}" src="{{ basePath }}/lib/bootstrap/js/bootstrap.min.js"></script>
    <script nonce="{{ nonce }}" src="{{ basePath }}/lib/datatables/dataTables.min.js"></script>
    <script nonce="{{ nonce }}" src="{{ basePath }}/lib/datatables/dataTables.bootstrap5.min.js"></script>

    <script nonce="{{ nonce }}" src="{{ basePath }}/js/functions.js"></script>
    {% block scripts %}{% endblock %}
</body>
</html>
